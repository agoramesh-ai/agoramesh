{
  "name": "@agentme/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for AgentMe - decentralized agent marketplace",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "e2e:demo": "tsx scripts/demo.ts"
  },
  "keywords": [
    "agents",
    "ai",
    "marketplace",
    "blockchain",
    "p2p",
    "a2a",
    "x402"
  ],
  "author": "AgentMe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentmecz/agentme"
  },
  "dependencies": {
    "@coinbase/x402": "^2.1.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.17.0",
    "express": "^5.2.1",
    "prettier": "^3.2.0",
    "supertest": "^7.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
