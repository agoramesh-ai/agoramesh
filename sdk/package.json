{
  "name": "@agentme/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for AgentMesh - decentralized agent marketplace",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "e2e:demo": "tsx scripts/e2e-demo.ts"
  },
  "keywords": [
    "agents",
    "ai",
    "marketplace",
    "blockchain",
    "p2p",
    "a2a",
    "x402"
  ],
  "author": "AgentMesh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timutti/agentmesh"
  },
  "dependencies": {
    "@coinbase/x402": "^2.1.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^25.0.0",
    "eslint": "^9.17.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
